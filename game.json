      "events": [
        {
          "type": "BuiltinCommonInstructions::JsCode",
          "inlineCode": [
            "/*",
            "Program name: vechten res.",
            "Author      : whmsft",
            "version     : 0.1b",
            "*/",
            "",
            "const _players = runtimeScene.getObjects(\"Player\");",
            "const player   = _players[0];",
            "const _bullets = runtimeScene.getObjects(\"Bullet\");",
            "const bullet   = _bullets[0];",
            "const LEFTKEY  = 37;",
            "const RIGHTKEY = 39;",
            "",
            "var x = runtimeScene.getGame().getInputManager().getCursorX()",
            "var y = runtimeScene.getGame().getInputManager().getCursorY()",
            "var mouse_pressed = runtimeScene.getGame().getInputManager().isMouseButtonPressed(0)",
            "var bullets = []",
            "",
            "// Check for movement triggers",
            "if (runtimeScene.getGame().getInputManager().isKeyPressed(RIGHTKEY) || ((((x > 88) && (x < 88+56)) && ((y > 566) && (y < 622))) && mouse_pressed)) {",
            "  player.setAnimation(0);",
            "  player.flipX(false);",
            "  player.setX(player.getX()+8)",
            "} else if (runtimeScene.getGame().getInputManager().isKeyPressed(LEFTKEY) || ((((x > 16) && (x < 72)) && ((y > 566) && (y < 622))) && mouse_pressed)) {",
            "  player.setAnimation(0);",
            "  player.flipX(true);",
            "  player.setX(player.getX()-8);",
            "} else {",
            "  player.setAnimation(1);",
            "}",
            "",
            "// Check for bullet triggers",
            "if ((((x > 288) && (x < 288+56)) && ((y > 566) && (y < 622))) && mouse_pressed) {",
            "  bullets.push(bullet)",
            "  bullets[-1].setX(player.getX())",
            "  bullets[-1].setX(player.getY())",
            "  runtimeScene.addObject(bullets[-1])",
            "}",
            "",
            "// Check for \"Player\" going out of screen",
            "if (player.getX() < -32) {player.setX(328);}",
            "if (player.getX() > 392) {player.setX(-32);}",
            ""
          ],
